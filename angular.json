{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "nostria-signer": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "angular",
      "sourceRoot": "angular/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "./angular/webpack.config.js",
              "mergeRules": {
                "externals": "replace"
              }
            },
            "allowedCommonJsDependencies": [
              "@scure/bip32",
              "@blockcore/blockcore-js",
              "buffer",
              "is-retry-allowed",
              "@stablelib/ed25519",
              "@stablelib/random",
              "elliptic",
              "s-sha3",
              "satcomma",
              "bitcoinjs-message",
              "err-code",
              "sparse-array"
            ],
            "aot": true,
            "outputPath": "dist/extension",
            "index": "angular/src/index.html",
            "main": "angular/src/main.ts",
            "polyfills": "angular/src/polyfills.ts",
            "tsConfig": "angular/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "styles": [
              "angular/src/styles/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "angular/src/styles/blockcore"
              ]
            },
            "scripts": [],
            "serviceWorker": true,
            "ngswConfigPath": "angular/ngsw-config.json"
          },
          "configurations": {
            "production": {
              "buildOptimizer": false,
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": false
                },
                "fonts": true
              },
              "vendorChunk": false,
              "extractLicenses": true,
              "sourceMap": false,
              "namedChunks": true,
              "outputHashing": "none",
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "6mb",
                  "maximumError": "16mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "assets": [
                {
                  "input": "angular/src/assets/blockcore/",
                  "output": "/assets",
                  "glob": "*"
                },
                {
                  "input": "angular/src/assets/i18n/",
                  "output": "/assets/i18n",
                  "glob": "*"
                },
                {
                  "input": "angular/src/manifest/blockcore/",
                  "output": "/",
                  "glob": "*"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "angular/src/environments/environment.ts",
                  "with": "angular/src/environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": true,
              "sourceMap": true,
              "namedChunks": true,
              "assets": [
                {
                  "input": "angular/src/assets/blockcore/",
                  "output": "/assets",
                  "glob": "*"
                },
                {
                  "input": "angular/src/assets/i18n/",
                  "output": "/assets/i18n",
                  "glob": "*"
                },
                {
                  "input": "angular/src/manifest/blockcore/",
                  "output": "/",
                  "glob": "*"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "options": {
            "port": 4201
          },
          "builder": "@angular-builders/custom-webpack:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "nostria-signer:build:production"
            },
            "development": {
              "browserTarget": "nostria-signer:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-builders/custom-webpack:extract-i18n",
          "options": {
            "browserTarget": "nostria-signer:build"
          }
        },
        "test": {
          "builder": "@angular-builders/custom-webpack:karma",
          "options": {
            "customWebpackConfig": {
              "path": "./angular/webpack.config.js",
              "mergeRules": {
                "externals": "replace"
              }
            },
            "main": "angular/src/test.ts",
            "polyfills": "angular/src/polyfills.ts",
            "tsConfig": "angular/tsconfig.spec.json",
            "karmaConfig": "angular/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "angular/src/favicon.ico",
              "angular/src/assets"
            ],
            "styles": [
              "angular/src/styles/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "angular/src/styles/blockcore"
              ]
            },
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false,
    "cache": {
      "enabled": false
    }
  }
}